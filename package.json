    "react-error-overlay": "^1.0.10",
    "react-icons-kit": "^1.0.7",
    "react-responsive-modal": "^2.0.0",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-toastify": "^4.0.0-rc.4",
